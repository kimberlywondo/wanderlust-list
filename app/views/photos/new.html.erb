<%= form_for([@trip, @photo]) do |f| %>

    <p>
      <%= f.label :description %>
      <%= f.text_area :description %>
    </p>
    <p>
      <%= f.label :photo_url %>
      <%= f.text_area :photo_url %>
    </p>

    <p>
      <%= f.submit %>
    </p>
		<%= link_to 'Add Photo', trip_path(@trip) %>

<% end %>